<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 圆角的半径 -->
    <corners android:radius="17dp"/>

    <!-- 填充颜色 -->
    <solid android:color="#eeeeee"/>

    <!-- 边框的宽度,每段虚线的长度，和两段虚线之间的间隔和颜色 -->
    <stroke
        android:width="2dp"
        android:dashWidth="0dp"
        android:dashGap="0dp"
        android:color="#333" />

</shape>